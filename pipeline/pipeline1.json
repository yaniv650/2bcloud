					}
				],
				"outputs": [
					{
						"referenceName": "BlobParquet",
						"type": "DatasetReference",
						"parameters": {
							"collection_name": "ServiceAreaCoord",
							"env": "test"
						}
					}
				]
			},
			{
				"name": "CopySynapseServiceAreaCoordinate",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowCopyCommand": true
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "_id",
									"type": "String"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "coordinates",
									"type": "String"
								},
								"sink": {
									"name": "coordinates"
								}
							},
							{
								"source": {
									"name": "updatedAt",
									"type": "String"
								},
								"sink": {
									"name": "updated_at",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "createdAt",
									"type": "String"
								},
								"sink": {
									"name": "created_at",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "siteId",
									"type": "String"
								},
								"sink": {
									"name": "site_id",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "BlobParquet",
						"type": "DatasetReference",
						"parameters": {
							"collection_name": "ServiceAreaCoord",
							"env": "test"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable",
						"type": "DatasetReference",
						"parameters": {
							"table": "stg_area_coord"
						}
					}
				]
			}
		],
		"annotations": []
	}
}