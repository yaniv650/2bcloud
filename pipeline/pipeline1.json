									"path": "$['createdAt']['$date']"
								},
								"sink": {
									"name": "createdAt",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['coordinates']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "CosmosCollection",
						"type": "DatasetReference",
						"parameters": {
							"collection_cosmos_name": "ServiceArea"
