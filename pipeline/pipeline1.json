{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "CopyParquetServiceAreaCoordinate",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbMongoDbApiSource",
						"batchSize": 100,
						"filter": ""
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['_id']"
								},
								"sink": {
									"name": "_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['coordinates']"
								},
								"sink": {
									"name": "coordinates",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['updatedAt']['$date']"
								},
								"sink": {
									"name": "updatedAt",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['site']['$id']"
								},
								"sink": {
									"name": "siteId",
