{
	"name": "BlobParquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"collection_name": {
				"type": "string"
			},
			"env": {
				"type": "string",
				"defaultValue": "test"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().collection_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@formatDateTime(utcnow(),'MM-dd-yyyy')",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@concat(dataset().env,'/','cosmosmrr')",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}